@using Apprenticeship.Entites

<a  href="/TeamLeader/Add" class="btn btn-primary">Create New Team Leader</a>
<table class="table table-striped">

    <tr>
        <th>
            First Name
        </th>
        <th>
            Second Name
        </th>
        <th>
            Third Name
        </th>
        <th>
            Last Name
        </th>
        <th>
           Email
        </th>
        <th>
            Company
        </th>
    </tr>
    @foreach (var t in (List<TeamLeader>)ViewBag.teamLeaders)
    {
        <tr>
            <td>@t.firstName</td>
            <td>@t.secondName</td>
            <td>@t.thirdName</td>
            <td>@t.lastName</td>
            <td>@t.Email</td>
            <td>@t.company.companyName</td>
        </tr>
    }
    

</table>





